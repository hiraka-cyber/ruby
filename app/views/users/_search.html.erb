<%= form_with url: search_users_path,method: :get,local: true do |f| %>
<div class="container">
  <div class="row">
    <div class="col-sm-10">
      <%= f.text_field :keyword ,placeholder:'please user name...',class:'form-control'%>
    </div>
    <div class="col-sm-2">
      <%= f.submit :search ,class:'btn btn-outline-primary'%>
    </div>
  </div>
</div>
<% end %>
